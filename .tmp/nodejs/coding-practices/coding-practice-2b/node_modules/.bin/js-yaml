#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/workspace/.tmp/nodejs/coding-practices/coding-practice-2b/node_modules/.pnpm/js-yaml@3.14.1/node_modules/js-yaml/bin/js-yaml.js/node_modules:/home/workspace/.tmp/nodejs/coding-practices/coding-practice-2b/node_modules/.pnpm/js-yaml@3.14.1/node_modules/js-yaml/bin/node_modules:/home/workspace/.tmp/nodejs/coding-practices/coding-practice-2b/node_modules/.pnpm/js-yaml@3.14.1/node_modules/js-yaml/node_modules:/home/workspace/.tmp/nodejs/coding-practices/coding-practice-2b/node_modules/.pnpm/js-yaml@3.14.1/node_modules:/home/workspace/.tmp/nodejs/coding-practices/coding-practice-2b/node_modules/.pnpm/node_modules:/home/workspace/.tmp/nodejs/coding-practices/coding-practice-2b/node_modules:/home/workspace/.tmp/nodejs/coding-practices/node_modules:/home/workspace/.tmp/nodejs/node_modules:/home/workspace/.tmp/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/.tmp/nodejs/coding-practices/coding-practice-2b/node_modules/js-yaml/bin/js-yaml.js/node_modules:/home/workspace/.tmp/nodejs/coding-practices/coding-practice-2b/node_modules/js-yaml/bin/node_modules:/home/workspace/.tmp/nodejs/coding-practices/coding-practice-2b/node_modules/js-yaml/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../js-yaml/bin/js-yaml.js" "$@"
else 
  exec node  "$basedir/../js-yaml/bin/js-yaml.js" "$@"
fi
