#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/workspace/.tmp/nodejs/coding-practices/coding-practice-2b/node_modules/.pnpm/@cnakazawa/watch@1.0.4/node_modules/@cnakazawa/watch/cli.js/node_modules:/home/workspace/.tmp/nodejs/coding-practices/coding-practice-2b/node_modules/.pnpm/@cnakazawa/watch@1.0.4/node_modules/@cnakazawa/watch/node_modules:/home/workspace/.tmp/nodejs/coding-practices/coding-practice-2b/node_modules/.pnpm/@cnakazawa/watch@1.0.4/node_modules/@cnakazawa/node_modules:/home/workspace/.tmp/nodejs/coding-practices/coding-practice-2b/node_modules/.pnpm/@cnakazawa/watch@1.0.4/node_modules:/home/workspace/.tmp/nodejs/coding-practices/coding-practice-2b/node_modules/.pnpm/@cnakazawa/node_modules:/home/workspace/.tmp/nodejs/coding-practices/coding-practice-2b/node_modules/.pnpm/node_modules:/home/workspace/.tmp/nodejs/coding-practices/coding-practice-2b/node_modules:/home/workspace/.tmp/nodejs/coding-practices/node_modules:/home/workspace/.tmp/nodejs/node_modules:/home/workspace/.tmp/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/.tmp/nodejs/coding-practices/coding-practice-2b/node_modules/@cnakazawa/watch/cli.js/node_modules:/home/workspace/.tmp/nodejs/coding-practices/coding-practice-2b/node_modules/@cnakazawa/watch/node_modules:/home/workspace/.tmp/nodejs/coding-practices/coding-practice-2b/node_modules/@cnakazawa/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@cnakazawa/watch/cli.js" "$@"
else 
  exec node  "$basedir/../@cnakazawa/watch/cli.js" "$@"
fi
