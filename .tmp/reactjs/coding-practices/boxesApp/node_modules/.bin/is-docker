#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/workspace/.tmp/reactjs/coding-practices/boxesApp/node_modules/.pnpm/is-docker@2.2.1/node_modules/is-docker/cli.js/node_modules:/home/workspace/.tmp/reactjs/coding-practices/boxesApp/node_modules/.pnpm/is-docker@2.2.1/node_modules/is-docker/node_modules:/home/workspace/.tmp/reactjs/coding-practices/boxesApp/node_modules/.pnpm/is-docker@2.2.1/node_modules:/home/workspace/.tmp/reactjs/coding-practices/boxesApp/node_modules/.pnpm/node_modules:/home/workspace/.tmp/reactjs/coding-practices/boxesApp/node_modules:/home/workspace/.tmp/reactjs/coding-practices/node_modules:/home/workspace/.tmp/reactjs/node_modules:/home/workspace/.tmp/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/.tmp/reactjs/coding-practices/boxesApp/node_modules/is-docker/cli.js/node_modules:/home/workspace/.tmp/reactjs/coding-practices/boxesApp/node_modules/is-docker/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../is-docker/cli.js" "$@"
else 
  exec node  "$basedir/../is-docker/cli.js" "$@"
fi
