#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/workspace/.tmp/reactjs/coding-practices/boxesApp/node_modules/.pnpm/cssesc@3.0.0/node_modules/cssesc/bin/cssesc/node_modules:/home/workspace/.tmp/reactjs/coding-practices/boxesApp/node_modules/.pnpm/cssesc@3.0.0/node_modules/cssesc/bin/node_modules:/home/workspace/.tmp/reactjs/coding-practices/boxesApp/node_modules/.pnpm/cssesc@3.0.0/node_modules/cssesc/node_modules:/home/workspace/.tmp/reactjs/coding-practices/boxesApp/node_modules/.pnpm/cssesc@3.0.0/node_modules:/home/workspace/.tmp/reactjs/coding-practices/boxesApp/node_modules/.pnpm/node_modules:/home/workspace/.tmp/reactjs/coding-practices/boxesApp/node_modules:/home/workspace/.tmp/reactjs/coding-practices/node_modules:/home/workspace/.tmp/reactjs/node_modules:/home/workspace/.tmp/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/.tmp/reactjs/coding-practices/boxesApp/node_modules/cssesc/bin/cssesc/node_modules:/home/workspace/.tmp/reactjs/coding-practices/boxesApp/node_modules/cssesc/bin/node_modules:/home/workspace/.tmp/reactjs/coding-practices/boxesApp/node_modules/cssesc/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../cssesc/bin/cssesc" "$@"
else 
  exec node  "$basedir/../cssesc/bin/cssesc" "$@"
fi
