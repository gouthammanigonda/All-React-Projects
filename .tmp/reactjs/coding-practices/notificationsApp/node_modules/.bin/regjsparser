#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/workspace/.tmp/reactjs/coding-practices/notificationsApp/node_modules/.pnpm/regjsparser@0.8.4/node_modules/regjsparser/bin/parser/node_modules:/home/workspace/.tmp/reactjs/coding-practices/notificationsApp/node_modules/.pnpm/regjsparser@0.8.4/node_modules/regjsparser/bin/node_modules:/home/workspace/.tmp/reactjs/coding-practices/notificationsApp/node_modules/.pnpm/regjsparser@0.8.4/node_modules/regjsparser/node_modules:/home/workspace/.tmp/reactjs/coding-practices/notificationsApp/node_modules/.pnpm/regjsparser@0.8.4/node_modules:/home/workspace/.tmp/reactjs/coding-practices/notificationsApp/node_modules/.pnpm/node_modules:/home/workspace/.tmp/reactjs/coding-practices/notificationsApp/node_modules:/home/workspace/.tmp/reactjs/coding-practices/node_modules:/home/workspace/.tmp/reactjs/node_modules:/home/workspace/.tmp/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/.tmp/reactjs/coding-practices/notificationsApp/node_modules/regjsparser/bin/parser/node_modules:/home/workspace/.tmp/reactjs/coding-practices/notificationsApp/node_modules/regjsparser/bin/node_modules:/home/workspace/.tmp/reactjs/coding-practices/notificationsApp/node_modules/regjsparser/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../regjsparser/bin/parser" "$@"
else 
  exec node  "$basedir/../regjsparser/bin/parser" "$@"
fi
