#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/workspace/.tmp/reactjs/coding-practices/notificationsApp/node_modules/.pnpm/svgo@1.3.2/node_modules/svgo/bin/svgo/node_modules:/home/workspace/.tmp/reactjs/coding-practices/notificationsApp/node_modules/.pnpm/svgo@1.3.2/node_modules/svgo/bin/node_modules:/home/workspace/.tmp/reactjs/coding-practices/notificationsApp/node_modules/.pnpm/svgo@1.3.2/node_modules/svgo/node_modules:/home/workspace/.tmp/reactjs/coding-practices/notificationsApp/node_modules/.pnpm/svgo@1.3.2/node_modules:/home/workspace/.tmp/reactjs/coding-practices/notificationsApp/node_modules/.pnpm/node_modules:/home/workspace/.tmp/reactjs/coding-practices/notificationsApp/node_modules:/home/workspace/.tmp/reactjs/coding-practices/node_modules:/home/workspace/.tmp/reactjs/node_modules:/home/workspace/.tmp/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/.tmp/reactjs/coding-practices/notificationsApp/node_modules/svgo/bin/svgo/node_modules:/home/workspace/.tmp/reactjs/coding-practices/notificationsApp/node_modules/svgo/bin/node_modules:/home/workspace/.tmp/reactjs/coding-practices/notificationsApp/node_modules/svgo/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../svgo/bin/svgo" "$@"
else 
  exec node  "$basedir/../svgo/bin/svgo" "$@"
fi
