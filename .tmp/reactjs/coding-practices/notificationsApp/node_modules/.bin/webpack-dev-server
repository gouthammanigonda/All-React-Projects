#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/workspace/.tmp/reactjs/coding-practices/notificationsApp/node_modules/.pnpm/webpack-dev-server@3.11.1_webpack@4.44.2/node_modules/webpack-dev-server/bin/webpack-dev-server.js/node_modules:/home/workspace/.tmp/reactjs/coding-practices/notificationsApp/node_modules/.pnpm/webpack-dev-server@3.11.1_webpack@4.44.2/node_modules/webpack-dev-server/bin/node_modules:/home/workspace/.tmp/reactjs/coding-practices/notificationsApp/node_modules/.pnpm/webpack-dev-server@3.11.1_webpack@4.44.2/node_modules/webpack-dev-server/node_modules:/home/workspace/.tmp/reactjs/coding-practices/notificationsApp/node_modules/.pnpm/webpack-dev-server@3.11.1_webpack@4.44.2/node_modules:/home/workspace/.tmp/reactjs/coding-practices/notificationsApp/node_modules/.pnpm/node_modules:/home/workspace/.tmp/reactjs/coding-practices/notificationsApp/node_modules:/home/workspace/.tmp/reactjs/coding-practices/node_modules:/home/workspace/.tmp/reactjs/node_modules:/home/workspace/.tmp/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/.tmp/reactjs/coding-practices/notificationsApp/node_modules/webpack-dev-server/bin/webpack-dev-server.js/node_modules:/home/workspace/.tmp/reactjs/coding-practices/notificationsApp/node_modules/webpack-dev-server/bin/node_modules:/home/workspace/.tmp/reactjs/coding-practices/notificationsApp/node_modules/webpack-dev-server/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../webpack-dev-server/bin/webpack-dev-server.js" "$@"
else 
  exec node  "$basedir/../webpack-dev-server/bin/webpack-dev-server.js" "$@"
fi
